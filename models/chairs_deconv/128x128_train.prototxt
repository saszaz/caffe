name: "CaffeNet"
layer {
  name: "data"
  type: "Data"
  top: "data"
  top: "label"  
  include {
    phase: TRAIN
  }
  data_param {
    # source: "data/chairs_data/chairs_128x128_reduced/data-lmdb"
    source: "data/chairs_data/chairs_113x113_reduced/data-lmdb"
    batch_size: 64
    scale: 0.00390625
    backend: LMDB
  }
}
layer {
  name: "angles"
  type: "HDF5Data"
  top: "angles"
  top: "labels"
  include {
    phase: TRAIN
  }
  hdf5_data_param {
    source: "deconv_test/chairs_data/angles_reduced_shuffle_mod.txt"
    batch_size: 64
  }
}
layer {
  name: "fc1_label"
  type: "Embed"
  bottom: "label"
  top: "fc1_label"
  param {
    lr_mult: 1
    decay_mult: 0
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  embed_param {
    num_output: 512
    input_dim: 843
    weight_filler {
      type: "gaussian"
      mean: 0.
      std: 1.
    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
}

